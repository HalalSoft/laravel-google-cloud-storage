{
  "name": "halalsoft/laravel-google-cloud-storage",
  "description": "A Google Cloud Storage flysystem for Laravel",
  "license": "MIT",
  "type": "library",
  "keywords": [
    "laravel",
    "gcs",
    "flysystem",
    "google",
    "storage"
  ],
  "authors": [
    {
      "name": "Dyas Yaskur",
      "email": "dyas@yaskur.com"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "illuminate/support": "*",
    "illuminate/filesystem": "^5.1|^6.0|^7.0",
    "league/flysystem-cached-adapter": "^1.0",
    "league/flysystem-aws-s3-v3": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Halalsoft\\LaravelGoogleCloudStorage\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Halalsoft\\LaravelGoogleCloudStorage\\GoogleCloudStorageServiceProvider"
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": ">=4.8"
  },
  "minimum-stability": "dev"
}
